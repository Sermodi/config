Description='A wireless connection using a custom network block configuration'
Connection='wireless'
Interface='wlp7s0'
ESSID='eduroam'
IP='dhcp'
Priority=1

Security=wpa-configsection
WPAConfigSection=(
    'ssid="eduroam"'
    'key_mgmt=WPA-EAP'
    'eap=TTLS'
    'group=TKIP'
    'pairwise=TKIP CCMP'
    'proto=WPA RSN'
    'anonymous_identity="anonymous@uva.es"'
    'identity="eDNI@uva.es"'
    'password="pass"'
    'phase2="auth=PAP"'
)
